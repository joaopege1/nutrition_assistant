[run]
source = .
omit =
    */tests/*
    */test/*
    */__pycache__/*
    */venv/*
    */env/*
    */node_modules/*
    */frontend/*
    */alembic/*
    setup.py
    conftest.py

[report]
precision = 2
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod

[html]
directory = htmlcov

